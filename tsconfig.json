{
  "compilerOptions": {
    "target": "ES5",
    "rootDir": "./src",
    "outDir": "./prod",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "resolveJsonModule": true,
    "noImplicitAny": false,
    "strictPropertyInitialization": false
  }
}
